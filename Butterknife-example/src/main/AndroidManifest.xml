<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mengdd.butterknifesample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleActivity"
            android:label="@string/title_activity_simple"></activity>
        <activity
            android:name=".SimpleFragmentActivity"
            android:label="@string/title_activity_simple_fragment"></activity>
        <activity
            android:name=".ListViewActivity"
            android:label="@string/title_activity_list_view"></activity>
        <activity
            android:name=".ViewListActivity"
            android:label="@string/title_activity_view_lists"></activity>
        <activity
            android:name=".SpinnerActivity"
            android:label="@string/title_activity_spinner"></activity>
    </application>

</manifest>
